<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StudyN.Resources.Dictionary3">
    <!--High Contrast Mode-->
    <Color x:Key="Primary">#000</Color>                     <!--Use for foreground elements like buttons-->
    <Color x:Key="PrimaryLight">#fff</Color>                <!--Use for background elements like page background-->
    <Color x:Key="NormalText">#0f0</Color>                  <!--Use for normal text-->
    <Color x:Key="NormalText2">#f00</Color>                 <!--Use for accents like button text-->
    <Color x:Key="NormalHeaderText">#00f</Color>            <!--Use for headers like label titles-->
    <Color x:Key="NormalLightText">#00f0f0</Color>          <!--Use for text meant to be overwritten or disabled, like in text fields-->
    <Color x:Key="DrawerTitleTextColor">#f0f000</Color>     <!---->
    <Color x:Key="TitleTextColor">#f000f0</Color>           <!--Use only for Page titles-->
</ResourceDictionary>